{
    "name": "akrabat/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "autoload": {
        "psr-4": {
            "App\\": "app/src",
            "Tests\\": "tests"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "illuminate/database": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
